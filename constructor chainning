import java.util.Scanner;
class student
{
    private String name;
    private int age;
    public student(String name)
    {
       this(name,40);
    }
    public student(String name,int age) {
        this.name = name;
        this.age = age;
    }
    public void display()
    {
        System.out.println("Name: "+name+" "+"age :"+age );
    }
}
public class Main
{
    public static void main(String[] args)
    {
       student s1=new student("tawfiq");
       s1.display();
    }
}
