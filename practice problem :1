//Area of a triangle:
import java.util.Scanner;
public class tawfiq
{
    public static void main(String[] args) {
       double pii=3.1416d;
       int a=4;
       int b=4;
       int d=4;
       double s=(double)(a+b+d)/2;
       double c=Math.sqrt(s*(s-a)*(s-b)*(s-d));
       System.out.println(c);
    }
}

