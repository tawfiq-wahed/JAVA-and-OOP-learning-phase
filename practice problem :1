// Area of a triangle

public class Main
{
    public static void main(String[] args)
    {
        int base=5;
        int height=3;
        double area=(double)0.5*base*height;
        System.out.println(area);
    }
}









//Area of a volume of ball

public class Main
{
    public static void main(String[] args)
    {
        int r=5;
        double pi=Math.PI;
        double res=(double)(4*pi*Math.pow(r,3))/3;
      System.out.println(res);
    }
}






//area of circle



public class Main
{
    public static void main(String[] args)
    {
        int radius=5;

        double area=(double)Math.PI*radius*radius;
        System.out.println(area);
    }
}


// convert the celcius scale into farenhite scale



public class Main
{
    public static void main(String[] args)
    {
        double celcius=99.00;

        double farenhite=(double)(((celcius/5)*9)+32);
        System.out.println(farenhite);
    }
}




//area of equilateral triangle

public class Main
{
    public static void main(String[] args)
    {
        int arm=5;

        double area=(double)(Math.sqrt(3)*arm*arm)/4;
        System.out.println(area);
    }
}


//Area of a triangle:
import java.util.Scanner;
public class tawfiq
{
    public static void main(String[] args) {
       double pii=3.1416d;
       int a=4;
       int b=4;
       int d=4;
       double s=(double)(a+b+d)/2;
       double c=Math.sqrt(s*(s-a)*(s-b)*(s-d));
       System.out.println(c);
    }
}




//Write a program to check whether a given input is a positive integer or not.

import java.util.Scanner;
public class Main
{
    public static void main(String[] args)
    {
        Scanner obj=new Scanner(System.in);
        int n=obj.nextInt();
       if(n>=0)System.out.println("it is positive number");
       else System.out.println("It is not a positive number");
    }
}



//Write a program to check whether a given input is an odd or even number.


import java.util.Scanner;
public class Main
{
    public static void main(String[] args)
    {
        Scanner obj=new Scanner(System.in);
        int n=obj.nextInt();
       if(n%2==0)System.out.println("it is even number");
       else System.out.println("It is a odd number");
    }
}



// create a calculator in specific operation:

import java.util.Scanner;
public class Main
{
    public static void main(String[] args)
    {
        Scanner obj=new Scanner(System.in);
        int n=obj.nextInt();
        int m =obj.nextInt();
       int l=n*m;
       int sum=n+m;
       double div=(double)(n/m);
       int sub=n-m;
        System.out.println("addition"+" "+sum);
        System.out.println("subtraction"+" "+sub);
        System.out.println("multiplication"+" "+l);
        System.out.println("division"+" "+div);
    }
}




//Grade calculator
import java.util.Scanner;
public class tawfiq
{
    public static void main(String[] args) {
      Scanner obj=new Scanner(System.in);
        int a=obj.nextInt();
      if(a>=0 &&a<=39)System.out.println("F");
     else if(a>=40 &&a<=59)System.out.println("C+");
     else if(a>=60 &&a<=69)System.out.println("B");
     else if(a>=70 &&a<=79)System.out.println("A-");
     else if(a>=80 &&a<=89)System.out.println("A");
     else System.out.println("A+");
    }
}

